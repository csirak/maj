type number: bool | int;

func assert(cond: bool): number {
    if (cond) {
        putchar('.');
        return 1;
    }
    return false;
}

func fact(n: int): void {
    if(n >= 20) {
        putchar('2');
    } else if(n >= 10){
        putchar('1');
    } else {
        putchar('0');
    }
}

func main(): void {
    var x = 20;
    var y = 10;
    var z = 5;

    fact(x);
    fact(y);
    fact(z);
}


